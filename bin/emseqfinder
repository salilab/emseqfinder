#!/usr/bin/env python
  
import IMP


def main():
    c = IMP.CommandDispatcher(
        short_help='Assign primary sequence to cryo-EM backbone.',
        long_help='Assignment of primary sequence to backbone fragments '
                  'traced in a cryo-EM map.',
        module_name="IMP.emseqfinder")
    c.main()

if __name__ == '__main__':
    main()
